{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "/var/log/twitch-bot.json",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 20971520,
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "Application": "TwitchBot.Service"
    }
  },
  "Twitch": {
    "Auth": {
      "Authority": "https://id.twitch.tv/oauth2",
      "ClientId": "<Set this in User Secrets>",
      "ClientSecret": "<Set this in User Secrets>",
      "AccessToken": "<Set this in User Secrets>"
    },
    "Chat": {
      "BotName": "vic10usBot",
      "Channel": "vic10usx",
      "PasswordGeneratorToken": "<Set this in User Secrets>",
      "AppToken": "<Set this in User Secrets>",
      "RespondToUnknownCommand": false
    },
    "IgnoredUsers": [ "nightbot", "pretzelrocks", "p4nth3rb0t", "vic10usBot" ],
